{
  "name": "@avocode/styled-avocode-email-tagsinput",
  "version": "2.1.5",
  "description": "React input component that handles creation and removal of tags based on matching email regex. Includes opinionated styles.",
  "keywords": [
    "react",
    "input",
    "element",
    "tag",
    "tags",
    "email",
    "slatejs",
    "avocode",
    "ui"
  ],
  "author": "Ondřej Synáček <ondrejsynacek@fastmail.com>",
  "license": "ISC",
  "main": "dist/styled-avocode-email-tagsinput.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "homepage": "https://github.com/avocode/avocode-email-tagsinput",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avocode/avocode-email-tagsinput.git"
  },
  "scripts": {
    "build": "npm run clean && npm run copy-public-flow-types && npm run flow:libdef && webpack --mode=production",
    "build:dev": "npm run clean && npm run copy-public-flow-types && npm run flow:libdef && webpack --mode=development",
    "build:watch": "webpack --mode=development --watch",
    "clean": "rm -rf dist && mkdir dist",
    "flow": " npm run flow:libdef && npm run copy-public-flow-types && flow",
    "flow:libdef": "flow-copy-source -v lib/ dist/",
    "flow:libdef:watch": "flow-copy-source -vw lib/ dist/",
    "copy-public-flow-types": "cp lib/types.js dist/",
    "prepublish": "npm run build",
    "profile": "webpack --mode=development --profile --json > stats.json"
  },
  "bugs": {
    "url": "https://github.com/avocode/avocode-email-tagsinput/issues"
  },
  "dependencies": {
    "@avocode/avocode-email-tagsinput": "^2.1.5",
    "classnames": "^2.2.6",
    "react-immutable-proptypes": "2.1.0",
    "slate-base64-serializer": "0.2.112",
    "slate-dev-environment": "0.2.2",
    "slate-hotkeys": "0.2.9",
    "slate-plain-serializer": "0.7.11",
    "slate-prop-types": "0.5.42",
    "tiny-invariant": "1.0.6"
  },
  "peerDependencies": {
    "immutable": "^3.8.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "@avocode/avocode-email-tagsinput-config": "^2.1.5",
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.7.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "flow-bin": "^0.126.1",
    "flow-copy-source": "^2.0.9",
    "immutable": "^3.8.1",
    "mini-css-extract-plugin": "^0.9.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
